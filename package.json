{
  "name": "mora-common",
  "version": "0.0.6",
  "description": "",
  "scripts": {
    "build": "./script.ts cpStyle && tsc",
    "release": "npm run build && npm publish && cnpm sync mora-common",
    "test": "jest"
  },
  "author": "Mora <qiuzhongleiabc@126.com> (https://qiu8310.github.io/)",
  "license": "MIT",
  "jest": {
    "roots": [
      "src"
    ],
    "mapCoverage": true,
    "collectCoverage": true,
    "coverageReporters": [
      "text",
      "html"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/helper/"
    ],
    "coverageDirectory": "./coverage",
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*\\.(test|spec))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "dependencies": {
    "core-decorators": "0.19.0",
    "mora-scripts": "~1.6.2",
    "react-side-effect": "1.1.3"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@types/core-decorators": "0.10.32",
    "@types/fs-extra": "3.0.3",
    "@types/jest": "19.2.4",
    "@types/node": "7.0.31",
    "@types/react": "15.0.29",
    "@types/react-dom": "15.5.0",
    "@types/react-transition-group": "1.1.0",
    "fs-extra": "3.0.1",
    "jest": "20.0.4",
    "jsdom": "11.0.0",
    "react-test-renderer": "15.6.1",
    "ts-jest": "20.0.6",
    "tslint": "5.4.3",
    "typescript": "2.3.4"
  }
}
